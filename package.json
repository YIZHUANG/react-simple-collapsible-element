{
  "name": "react-simple-collapsible-element",
  "version": "3.0.0",
  "description": "simple react collapsible supports nested elements",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/YIZHUANG/react-simple-collapsible-element.git"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint ./ --fix --ext .jsx --ext .js",
    "lint:fix": "eslint ./ --fix --ext .jsx --ext .js --fix",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "makeFolder": "mkdir dist",
    "makeFiles": "babel src --out-dir dist",
    "build": "npm run clean && npm run makeFolder && npm run makeFiles"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run test"
    }
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ]
  },
  "author": "YIZHUANG",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.5.8"
  },
  "keywords": [
    "collapsible",
    "react-component",
    "react",
    "FAQ",
    "collapse"
  ],
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-class-properties": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-minify": "^0.5.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rimraf": "^2.6.2",
    "webpack": "^2.5.1"
  }
}
